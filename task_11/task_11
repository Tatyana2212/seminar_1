// Выяснить, кратно ли число заданному, если нет, вывести остаток.
Console.Write("enter first: ");
double first = double.Parse(Console.ReadLine() ?? "0");
Console.Write("enter second:");
double second = double.Parse(Console.ReadLine() ?? "0");
double excess = (first % second);
if (  first % second == 0)
{
    Console.WriteLine("first number is  multiple to second ");
}
else
{
    Console.WriteLine($"first number is not multiple to second, residue {excess}");
}
